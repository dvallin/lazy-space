{
  "name": "lazy-space",
  "version": "0.4.3-SNAPSHOT",
  "description": "Monads and collections in typescript",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib/src/"
  ],
  "repository": "github:dvallin/lazy-space",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean-lib && tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "format:check": "prettier --config .prettierrc.yaml --list-different '{src,test}/**/*.{ts,tsx,js,jsx,html}'",
    "format": "prettier --config .prettierrc.yaml --write '{src,test}/**/*.{ts,tsx,js,jsx,html}'",
    "lint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "doc": "typedoc --mode file --theme minimal src/index.ts",
    "clean-lib": "rm -f -r -d lib"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "mockdate": "^3.0.5",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
